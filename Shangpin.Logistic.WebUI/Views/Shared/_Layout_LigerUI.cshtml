@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@Url.Content("~/content/images/favicon.ico")" type="image/x-icon" />
    <!--主框架颜色-->
    <link href="@Url.Content("~/Content/ligerUI/skins/Aqua/css/ligerui-all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/LigerUI/skins/Gray2014/css/all.css")" rel="stylesheet" type="text/css" />
    @*<link href="@Url.Content("~/Content/LigerUI/skins/Gray/css/all.css")" rel="stylesheet" type="text/css">*@
    <!--当前后台(首页)的样式-->
    <link href="@Url.Content("~/Content/common.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/index.css")" rel="stylesheet" type="text/css" />
    <!--主框架js-->
    <script src="@Url.Content("~/Scripts/jquery-1.8.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ligerUI/js/ligerui.min.js")" type="text/javascript"></script>
    <!--插件脚本-->
    <script src="@Url.Content("~/Content/ligerUI/js/plugins/ligerTab.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ligerUI/js/plugins/ligerLayout.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ligerUI/js/plugins/ligerForm.js")" type="text/javascript"></script>
    <!--封装的后台LigerUI友好的各种ajax请求函数-->
    <script src="@Url.Content("~/Content/ligerUI/js/LG.js")" type="text/javascript"></script>
    <script src="~/Content/ligerUI/js/ligerui.expand.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ligerUI/js/common.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/common/index.js")" type="text/javascript"></script>*@
    <script src="@Url.Content("~/Scripts/common/json2.js")" type="text/javascript"></script>
    @*弹出框*@
    <link href="@Url.Content("~/Scripts/plugins/ymPrompt/skin/simple_gray/ymPrompt.css")" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/ymPrompt/ymPrompt.min.js")"></script>
    @*<script src="@Url.Content("~/Content/ligerUI/js/core/base.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ligerUI/js/plugins/ligerDrag.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/ligerUI/js/plugins/ligerDialog.js")" type="text/javascript"></script>*@
    @RenderSection("HeaderScripts", false)
</head>
<body>
    <div id="pageloading" style="display: block;"></div>
    @RenderBody()
    @RenderSection("FooterScripts", false)
</body>
</html>
