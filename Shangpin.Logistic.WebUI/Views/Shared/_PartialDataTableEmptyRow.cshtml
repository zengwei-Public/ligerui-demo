@model System.Collections.IList
@{
    if (ViewBag.ColumnCount == null) { ViewBag.ColumnCount = 100; }
}
@if (Model == null)
{
    <tr class="trEmpty">
        <td colspan="@ViewBag.ColumnCount" style="line-height:@ViewData["height"]px;">
            请输入探索条件进行查询！
        </td>
    </tr>
}
else if (Model.Count == 0)
{
    <tr class="trNoData">
        <td colspan="@ViewBag.ColumnCount" style="line-height:@ViewData["height"]px;">
            没有可显示的数据！
        </td>
    </tr>
}